{
  "name": "Simulated-Memory",
  "version": "0.1.0",
  "author": "Dylan Clement",
  "description": "Simulated Memory",
  "main": "./server.js",
  "dependencies": {
    "async": "0.7.x",
    "express": "3.1.x",
    "connect-assets": "2.4.x",
    "jade": "1.3.x",
    "neo4j": "1.1.x",
    "less": "1.7.x",
    "lodash": "2.4.x",
    "moment": "2.5.x"
  },
  "noAnalyze": true,
  "license": "Apache",
  "devDependencies": {
    "bunyan": "0.22.x",
    "mocha": "1.18.x",
    "chai": "1.9.x",
    "coffee-script": "1.7.x",
    "supervisor": "0.6.x",
    "npm": "1.4.x",
    "node-inspector": "0.7.x",
    "gulp": "~3.6.1",
    "gulp-watch": "~0.6.2",
    "gulp-coffee": "~1.4.2",
    "gulp-less": "~1.2.3",
    "gulp-concat": "~2.2.0",
    "gulp-rename": "~1.2.0",
    "gulp-uglify": "~0.2.1",
    "gulp-coffeelint": "~0.3.2",
    "tiny-lr": "0.0.5",
    "gulp-jade": "~0.5.0",
    "gulp-supervisor": "~0.1.1",
    "gulp-clean": "~0.2.4"
  },
  "scripts": {
    "start": "coffee ./server/src/app.coffee | bunyan",
    "watch": "supervisor -i src/public,src/views -e js,coffee,json -w src server | bunyan",
    "debug": "npm run inspector; node --debug server | bunyan",
    "inspector": "if [[ ! $(ps | grep -v grep | grep node-inspector) ]]; then (node-inspector &); fi",
    "test": "./node_modules/mocha/bin/mocha --compilers coffee:coffee-script",
    "migrate": "make migrate"
  },
  "engines": {
    "node": "0.11.11",
    "npm": "1.4.x"
  }
}
